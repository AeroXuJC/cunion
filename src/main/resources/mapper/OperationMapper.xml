<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cunion.mapper.OperationMapper">
    <select id="searchAllOperation" resultType="HashMap">
        select * from operation where is_deleted = 0 order by create_time desc;
    </select>
</mapper>
